{
  "name": "@testa-lerna/lib2-utils",
  "version": "0.0.4",
  "author": "Mariano Heller",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/lib2-utils.esm.js",
  "size-limit": [
    {
      "path": "dist/lib2-utils.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/lib2-utils.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.6.0",
    "@types/lodash": "^4.14.161",
    "husky": "^4.3.0",
    "size-limit": "^4.6.0",
    "tsdx": "^0.14.0",
    "tslib": "^2.0.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "lodash": "^4.17.20"
  },
  "gitHead": "51cc3ffdd3d0cc2f73f40c0d9012f7f79f939135"
}
